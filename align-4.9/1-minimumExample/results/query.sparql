PREFIX ns0:<https://w3id.org/i40/aml2#>
PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl:<http://www.w3.org/2002/07/owl#>
PREFIX ns1:<https://w3id.org/i40/aml#>
PREFIX xsd:<http://www.w3.org/2001/XMLSchema#>

CONSTRUCT { ?s1 owl:sameAs ?s2 }
WHERE {
?s1 rdf:type ns0:ExternalReference .
{
?s2 rdf:type ns1:ExternalReference .
}
?s1 ns0:refBaseClassPath ?o3 .
?s2 ns1:refBaseClassPath ?o5 .
FILTER ( lcase(str(?o3)) = lcase(str(?o5)) )
}


